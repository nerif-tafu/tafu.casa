{
  "name": "tafu.casa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "setup": "node scripts/setup-dev.js",
    "prepare": "node -e \"try { require('./scripts/setup-dev.js') } catch(e) { console.log('Skipping cert setup in container') }\"",
    "dev-client": "docker-compose -f docker-compose.client.yml up --build",
    "dev-client-ssl": "docker-compose -f docker-compose.client-ssl.yml up --build",
    "dev-server": "docker-compose -f docker-compose.server.yml up --build",
    "dev-server-ssl": "docker-compose -f docker-compose.server-ssl.yml up --build",
    "docker:down": "docker-compose down"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "autoprefixer": "^10.4.20",
    "mediasoup-client": "^3.7.18",
    "postcss": "^8.4.49",
    "socket.io-client": "^4.7.4",
    "svelte": "^5.8.0",
    "tailwindcss": "^3.4.16",
    "video.js": "^8.21.0",
    "vite": "^6.0.3"
  }
}
